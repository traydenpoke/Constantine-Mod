		<!--Diplomacy-->
		<!--FIRST MEET-->
		<!--First AI line-->
		<Row Tag="LOC_DIPLO_FIRST_MEET_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Hello there!</Text>
		</Row>
		<!--AI invitation to visit nearby City-->
		<Row Tag="LOC_DIPLO_FIRST_MEET_VISIT_RECIPIENT_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Do you want to visit Rome?</Text>
		</Row>
		<!--AI accepts Human invitation-->
		<Row Tag="LOC_DIPLO_FIRST_MEET_NEAR_INITIATOR_POSITIVE_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Let me come and have a look, perhaps spread a little Christianity...</Text>
		</Row>
		<!--AI invitation to exchange Capital Information-->
		<Row Tag="LOC_DIPLO_FIRST_MEET_NO_MANS_INFO_EXCHANGE_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Let's exchange information!</Text>
		</Row>
		<!--GREETINGS-->
		<Row Tag="LOC_DIPLO_GREETING_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Need help with anything?</Text>
		</Row>
		<!--DELEGATION-->
		<!--AI Accepts-->
		<Row Tag="LOC_DIPLO_ACCEPT_DELEGATION_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Thank you for your money, have a nice day.</Text>
		</Row>
		<!--AI Rejects-->
		<Row Tag="LOC_DIPLO_REJECT_DELEGATION_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>We are closed.</Text>
		</Row>
		<!--AI Requests -->
		<Row Tag="LOC_DIPLO_DELEGATION_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>We have delivered a shipment for you.</Text>
		</Row>
		<!--OPEN BORDERS-->
		<!--AI accepts from human-->
		<Row Tag="LOC_DIPLO_ACCEPT_OPEN_BORDERS_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>We will accept this contract.</Text>
		</Row>
		<!--AI rejects from human-->
		<Row Tag="LOC_DIPLO_REJECT_OPEN_BORDERS_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Your contract does not interest us. </Text>
		</Row>
		<!--AI requests from human-->
		<Row Tag="LOC_DIPLO_OPEN_BORDERS_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>We propose a contract.</Text>
		</Row>
		<!--DECLARE FRIENDSHIP-->
		<!--AI accepts from human-->
		<Row Tag="LOC_DIPLO_ACCEPT_DECLARE_FRIEND_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Closer relations is in our best interest at this time.</Text>
		</Row>
		<!--AI Rejects from human -->
		<Row Tag="LOC_DIPLO_REJECT_DECLARE_FRIEND_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Closer relations is not in our interests.</Text>
		</Row>
		<!--AI Requests friendship from human-->
		<Row Tag="LOC_DIPLO_DECLARE_FRIEND_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>I propose closer relations. What say you?</Text>
		</Row>
		<!--Human accepts AI requests, AI responds-->
		<Row Tag="LOC_DIPLO_ACCEPT_DECLARE_FRIEND_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Glad that you agree.</Text>
		</Row>
		<!--Human rejects AI requests, AI responds-->
		<Row Tag="LOC_DIPLO_REJECT_DECLARE_FRIEND_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>You wouldn't have helped us anyways.</Text>
		</Row>
		<!--ALLIANCE-->
		<!--AI Requests an alliance from human-->
		<Row Tag="LOC_DIPLO_MAKE_ALLIANCE_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>I propose an alliance.</Text>
		</Row>
		<!--KUDOS & WARNINGS-->
		<!--AI Kudos-->
		<Row Tag="LOC_DIPLO_KUDO_EXIT_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Our interest are similar. Good.</Text>
		</Row>
		<!--AI Warnings-->
		<Row Tag="LOC_DIPLO_WARNING_EXIT_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Your interest are conflicting with ours. Be careful.</Text>
		</Row>
		<!--AI warns player for border troops-->
		<Row Tag="LOC_DIPLO_WARNING_TOO_MANY_TROOPS_NEAR_ME_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>You have too many armed associates on our borders. What's the meaning of this?</Text>
		</Row>
		<!--TRADE-->
		<!--AI Accepts Deal-->
		<Row Tag="LOC_DIPLO_ACCEPT_MAKE_DEAL_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>{LOC_DIPLO_ACCEPT_DECLARE_FRIEND_FROM_AI_TB_LEADER_CONSTANTINE_ANY}</Text>
		</Row>
		<!--AI Rejects Deal-->
		<Row Tag="LOC_DIPLO_REJECT_MAKE_DEAL_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>{LOC_DIPLO_REJECT_DECLARE_FRIEND_FROM_AI_TB_LEADER_CONSTANTINE_ANY}</Text>
		</Row>
		<!--DENOUNCE-->
		<!--From Human-->
		<Row Tag="LOC_DIPLO_DENOUNCE_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>That was a mistake. The waters ahead aren't calm.</Text>
		</Row>
		<!--From AI-->
		<Row Tag="LOC_DIPLO_DENOUNCE_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Your interest are too far from our own and we will not stand for it! (Denounces You)</Text>
		</Row>
		<!--DECLARE WAR-->
		<!--Human Declares War-->
		<Row Tag="LOC_DIPLO_DECLARE_WAR_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Our interests conflict. Time to add you to the monopoly!</Text>
		</Row>
		<!--AI Declares War-->
		<Row Tag="LOC_DIPLO_DECLARE_WAR_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>Our interests conflict. Time to add you to the monopoly!</Text>
		</Row>
		<!--MAKE PEACE-->
		<!--AI accepts from human-->
		<Row Tag="LOC_DIPLO_MAKE_PEACE_AI_ACCEPT_DEAL_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>This suits our interests.</Text>
		</Row>
		<!--AI refuses human-->
		<Row Tag="LOC_DIPLO_MAKE_PEACE_AI_REFUSE_DEAL_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>We refuse your peace contract!</Text>
		</Row>
		<!--AI requests from human-->
		<Row Tag="LOC_DIPLO_MAKE_PEACE_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>We ask for a peace contract.</Text>
		</Row>
		<!--DEFEAT-->
		<!--AI is Defeated-->
		<Row Tag="LOC_DIPLO_DEFEAT_FROM_AI_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>You cheated... you must have... you damn Catholic!</Text>
		</Row>
		<!--Human is Defeated (will see this in hotseat)-->
		<Row Tag="LOC_DIPLO_DEFEAT_FROM_HUMAN_TB_LEADER_CONSTANTINE_ANY" Language="en_US">
			<Text>[LOC_DIPLO_DEFEAT_FROM_AI_TB_LEADER_CONSTANTINE_ANY]</Text>
		</Row>




		
		
		
		
		
		

